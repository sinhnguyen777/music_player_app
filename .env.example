# Firebase Configuration Template
# Copy file này thành .env và điền các giá trị thực tế từ Firebase Console

# Firebase Project Config
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_STORAGE_BUCKET=your_storage_bucket
FIREBASE_AUTH_DOMAIN=your_auth_domain

# Firebase API Keys
FIREBASE_WEB_API_KEY=your_web_api_key
FIREBASE_ANDROID_API_KEY=your_android_api_key
FIREBASE_IOS_API_KEY=your_ios_api_key

# Firebase App IDs
FIREBASE_WEB_APP_ID=your_web_app_id
FIREBASE_ANDROID_APP_ID=your_android_app_id
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_WINDOWS_APP_ID=your_windows_app_id

# Firebase Measurement IDs
FIREBASE_WEB_MEASUREMENT_ID=your_web_measurement_id
FIREBASE_WINDOWS_MEASUREMENT_ID=your_windows_measurement_id

# iOS Bundle ID
IOS_BUNDLE_ID=com.example.musicPlayerApp